{
  "name": "frontend",
  "version": "2.5.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rimraf ./build",
    "cleanBuild": "npm run clean && npm run build",
    "deepClean": "rimraf package-lock.json ./build ./node_modules",
    "deepCleanRebuild": "npm run deepClean && npm install && npm run build",
    "checkDependencies": "npx npm-check-updates",
    "updateDependencies": "npx npm-check-updates -u && npm install & npm run cleanBuild",
    "install:react": "npm install react@latest react-dom@latest react-router-dom@latest react-scripts@latest react-table@latest",
    "install:emotion": "npm install @emotion/react@latest @emotion/styled@latest",
    "install:muimaterial": "npm install @mui/material@5.16.7 @mui/icons-material@5.16.7",
    "install:roboto": "npm install @fontsource/roboto@latest",
    "install:qrcode": "npm install qrcode.react@latest",
    "install:rjsf": "npm install @rjsf/core@latest @rjsf/utils@latest @rjsf/validator-ajv8@latest @rjsf/mui@latest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@rjsf/core": "^5.22.4",
    "@rjsf/mui": "^5.22.4",
    "@rjsf/utils": "^5.22.4",
    "@rjsf/validator-ajv8": "^5.22.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "notistack": "^2.0.8",
    "qrcode.react": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "react-scripts": "^5.0.1",
    "react-table": "^7.8.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
  }
}
