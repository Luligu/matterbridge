{
  "name": "frontend",
  "version": "2.8.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "watch": "vite build --watch",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint .",
    "eslint": "eslint \"frontend/src/**/*.{ts,tsx,js,jsx}\"",
    "test": "vitest run",
    "test:watch": "vitest --reporter verbose",
    "test:verbose": "vitest run --reporter verbose",
    "test:coverage": "vitest run --reporter verbose --coverage",
    "clean": "npx rimraf ./build",
    "cleanBuild": "npm run clean && npm run build",
    "deepClean": "npx rimraf package-lock.json ./build ./node_modules",
    "deepCleanRebuild": "npm run deepClean && npm install && npm run build && npm run test",
    "checkDependencies": "npx npm-check-updates",
    "updateDependencies": "npx npm-check-updates -u && npm install & npm run cleanBuild",
    "reset": "npm run deepClean && npm run install:all && npm run build && npm run test",
    "install:all": "npm run install:react && npm run install:emotion && npm run install:muimaterial && npm run install:mdi && npm run install:qrcode && npm run install:notistack && npm run install:rjsf && npm run install:react-table && npm run install:typescript && npm run install:vite && npm run install:vitest",
    "install:react": "npm install react react-dom react-router react-router-dom --omit=dev",
    "install:emotion": "npm install @emotion/react @emotion/styled --omit=dev",
    "install:muimaterial": "npm install @mui/material @mui/icons-material --omit=dev",
    "install:mdi": "npm install @mdi/js @mdi/react --omit=dev",
    "install:qrcode": "npm install qrcode.react --omit=dev",
    "install:notistack": "npm install notistack --omit=dev",
    "install:rjsf": "npm install @rjsf/core @rjsf/utils @rjsf/validator-ajv8 --omit=dev",
    "install:react-table": "npm install react-table@latest --omit=dev",
    "install:typescript": "npm install --save-dev typescript @types/node",
    "install:vite": "npm install --save-dev vite @vitejs/plugin-react",
    "install:vitest": "npm install --save-dev vitest jsdom @testing-library/react @testing-library/jest-dom",
    "install:eslint": "npm install --save-dev eslint eslint-plugin-react eslint-plugin-react-hooks @typescript-eslint/eslint-plugin @typescript-eslint/parser"
  },
  "overrides": {
    "react-table": {
      "react": "^19.0.0"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@rjsf/core": "^5.24.13",
    "@rjsf/utils": "^5.24.13",
    "@rjsf/validator-ajv8": "^5.24.13",
    "notistack": "^3.0.2",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.1",
    "react-router-dom": "^7.9.1",
    "react-table": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.5.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^27.0.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  }
}