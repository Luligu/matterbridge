{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "vitest/**/*.ts",
    "vitest/**/*.tsx"
  ],
}